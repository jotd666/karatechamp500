* main "task" scheduler
* loop until one of the 2 timers/counters is nonzero
* jump at different locations
scheduler_loop_b493:
	lea	unknown_C006,a0                	| [$b493: ld   hl,unknown_C006]
	tst.b	nb_tasks_to_start_C029                 	| [$b496: ld   a,(nb_tasks_to_start_C029)]
											| [$b499: and  a]
