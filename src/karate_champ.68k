	.endif

karate_champ_irq:
	bra		on_periodic_interrupt_B72F
	
karate_champ_reset:
	* default: 1 update/irq call
	* we need 60 updates / second so on amiga PAL
	* we need to set this variable to 2 once out of 5 calls
	move.b	#1,vb_ticks_count
	lea		stack_top_CF00,sp
l_0000:
	lea		active_task_bit_table_C000,a0
	bsr		osd_break

	jra	startup_B045                       	| [jp   startup_B045]
	
store_a0_in_player_structure_1B_address:
	move.l	a0,d5
	sub.l	#table_E008,d5
	move.b	d5,(0x1c,a3)                    	| [$e073: ld   (iy+$1c),h]  LSB of offset
	lsr.w	#8,d5
	move.b	d5,(0x1b,a3)                    	| [$e070: ld   (iy+$1b),l]  MSB of offset
	rts
	
store_a1_in_player_structure_1B_address:
	move.l	a0,d3
	sub.l	#table_E008,d3
	move.b	d3,(0x1c,a3)                    	| [$e073: ld   (iy+$1c),h]  LSB of offset
	lsr.w	#8,d3
	move.b	d3,(0x1b,a3)                    	| [$e070: ld   (iy+$1b),l]  MSB of offset
	rts
	
load_player_structure_1B_address_to_a0:
	move.b	(0x1b,a3),d5                    	| [$e079: ld   l,(iy+$1b)]  MSB of offset
	lsl.w	#8,d5
	move.b	(0x1c,a3),d5                    	| [$e07c: ld   h,(iy+$1c)]  LSB of offset
	add.l	#table_E008,d5
	move.l	d5,a0
	rts

